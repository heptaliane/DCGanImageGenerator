{
  "dataset": {
    "train": {
      "dirname": "data/train",
      "ext": ".jpg"
    },
    "test": {
      "dirname": "data/test",
      "ext": ".jpg"
    }
  },
  "loader": {
    "batch_size": 8,
    "num_workers": 10,
    "pin_memory": true,
    "drop_last": true
  },
  "model": {
    "in_ch": 100,
    "out_ch": 3,
    "depth": 4,
    "detach": -1,
    "pretrained": {
      "generator": null,
      "discriminator": null
    }
  },
  "optimizer": {
    "generator": {
      "lr": 1e-4,
      "betas": [0.5, 0.999]
    },
    "discriminator": {
      "lr": 1e-4,
      "betas": [0.5, 0.999]
    }
  },
  "evaluator_interval": {
    "image": 1,
    "snapshot": 10
  },
  "iteration_per_epoch": 1000,
  "seed": 1234
}
